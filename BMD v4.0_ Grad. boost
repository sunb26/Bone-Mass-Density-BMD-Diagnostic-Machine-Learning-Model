\n","def norm(x):\n","  return (x - train_stats['mean']) / train_stats['std']\n","\n","x_train_normalized = norm(X_train)\n","x_test_normalized = norm(X_test)"],"execution_count":6,"outputs":[]},{"cell_type":"code","metadata":{"id":"H6vVa43UVzXJ","colab_type":"code","colab":{}},"source":["from imblearn.over_sampling import ADASYN\n","\n","column_names = ['Age','Sex','L1','L2','L3','L4', 'R1','R2', 'R3','Lf1','Lf2','Lf3']\n","\n","ada = ADASYN()\n","X_res, Y_res = ada.fit_resample(X_train,Y_train)\n","\n","y_res = pd.DataFrame(Y_res, columns=['Diagnosis'])\n","x_res = pd.DataFrame(X_res, columns=column_names)\n","\n","#Resampling caused the 'Sex' data points to contain values between 0 and 1. This function rounds the values to either or\n","def edit_sex(x):\n","  if x >0.5:\n","    x = 1\n","    return x\n","  elif x<0.5:\n","    x=0\n","    return x\n","\n","x_res['Sex'] = x_res['Sex'].apply(edit_sex)\n","\n","\n","train_stats = x_res.describe()\n","train_stats = train_stats.transpose()\n","\n","x_train_normalized = norm(x_res)\n","x_test_normalized = norm(X_test)"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"0YE0hdIlV_lJ","colab_type":"code","colab":{"base_uri":"https://localhost:8080/","height":35},"executionInfo":{"status":"ok","timestamp":1595015698773,"user_tz":240,"elapsed":514,"user":{"displayName":"Benjamin Sun","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14Gjk-X-xY0Shww4LJnya3t7kmkZ_jdFshpN7QCgydQ=s64","userId":"07662009587488003776"}},"outputId":"b4a1d8b6-e245-4f6f-dc06-123b547ac1c1"},"source":["unique, count = np.unique(y_res, return_counts=True)\n","y_train_value_count = {k:v for (k,v) in zip(unique,count)}\n","y_train_value_count"],"execution_count":8,"outputs":[{"output_type":"execute_result","data":{"text/plain":["{0: 10434, 1: 10165, 2: 10227}"]},"metadata":{"tags":[]},"execution_count":8}]},{"cell_type":"code","metadata":{"id":"kmRi4w_MWCLH","colab_type":"code","colab":{"base_uri":"https://localhost:8080/","hei